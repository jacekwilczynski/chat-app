{
  "name": "chat-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.3",
    "lodash.flow": "^3.5.0",
    "polished": "^1.9.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0",
    "styled-components": "^3.4.0",
    "ws": "^6.0.0"
  },
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server-once": "NODE_ENV=development NODE_PATH=server babel-node server",
    "server": "nodemon --exec npm run server-once",
    "client": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "concurrently": "^3.6.1",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.0",
    "prop-types": "latest",
    "redux-logger": "^3.0.6"
  },
  "proxy": "http://localhost:5000/"
}
